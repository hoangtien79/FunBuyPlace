
{
  "expo": {
    "name": "FunBuy",
    "slug": "FunBuy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/6d071ef7-2761-4f5b-b768-3fd9cd4fe92f.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/6d071ef7-2761-4f5b-b768-3fd9cd4fe92f.png",
      "resizeMode": "contain",
      "backgroundColor": "#4ECDC4"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.funbuy.marketplace",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/6d071ef7-2761-4f5b-b768-3fd9cd4fe92f.png",
        "backgroundColor": "#4ECDC4"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.funbuy.marketplace"
    },
    "web": {
      "favicon": "./assets/images/6d071ef7-2761-4f5b-b768-3fd9cd4fe92f.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "funbuy",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
